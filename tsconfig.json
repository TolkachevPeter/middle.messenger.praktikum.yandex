{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es2019",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true,

  }
}
